{
    "name": "@dehn/dehn-risk-planner",
    "version": "1.0.2",
    "scripts": {
        "dev": "vite --port=4200 --force",
        "build": "vue-tsc --noEmit && vite build",
        "postbuild": "rm dist/mockServiceWorker.js",
        "build:test": "vue-tsc --noEmit && vite build --mode=test",
        "preview": "vite preview --port 5050",
        "test:unit": "vitest --environment jsdom",
        "test:unit:ci": "vitest run --environment jsdom",
        "coverage": "vitest run --coverage --environment jsdom",
        "test:e2e": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress open'",
        "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress run'",
        "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
    },
    "publishConfig": {
        "@dehn:registry": "https://npm.pkg.github.com/"
    },
    "dependencies": {
        "@dehn/dehn-risk-norm-lib": "^1.0.1-64c",
        "@dehn/risk-api-client": "0.0.1-6",
        "@dehn/risk-api-mock": "1.0.0",
        "@meforma/vue-toaster": "1.3.0",
        "@vueuse/core": "^10.9.0",
        "clientjs": "^0.2.1",
        "file-saver": "2.0.5",
        "jwt-decode": "^4.0.0",
        "keycloak-js": "^24.0.4",
        "marked": "^12.0.2",
        "pinia": "^2.1.7",
        "pinia-plugin-persistedstate": "^3.2.1",
        "tw-elements": "1.0.0-alpha13",
        "vue": "^3.4.26",
        "vue-currency-input": "^3.1.0",
        "vue-i18n": "^9.13.1",
        "vue-router": "^4.3.2"
    },
    "devDependencies": {
        "@faker-js/faker": "^8.4.1",
        "@intlify/vite-plugin-vue-i18n": "^7.0.0",
        "@pinia/testing": "^0.1.3",
        "@rushstack/eslint-patch": "^1.10.2",
        "@tailwindcss/typography": "0.5.13",
        "@testing-library/vue": "^8.0.3",
        "@types/jsdom": "^21.1.6",
        "@types/node": "^20.12.9",
        "@vitejs/plugin-vue": "2.3.4",
        "@vue/eslint-config-prettier": "^9.0.0",
        "@vue/eslint-config-typescript": "10.0.0",
        "@vue/test-utils": "^2.4.5",
        "@vue/tsconfig": "0.1.3",
        "autoprefixer": "^10.4.19",
        "axios": "^1.6.8",
        "c8": "^9.1.0",
        "cypress": "^13.8.1",
        "eslint": "8.56.0",
        "eslint-plugin-cypress": "^3.1.1",
        "eslint-plugin-vue": "8.2.0",
        "jsdom": "^24.0.0",
        "msw": "^1.3.3",
        "postcss": "^8.4.38",
        "prettier": "^3.2.5",
        "rollup-plugin-analyzer": "4.0.0",
        "sass": "^1.76.0",
        "start-server-and-test": "^2.0.3",
        "tailwindcss": "^3.4.3",
        "typescript": "^5.4.5",
        "vite": "2.9.18",
        "vitest": "^1.6.0",
        "vue-router-mock": "^1.1.0",
        "vue-tsc": "^2.0.16"
    },
    "msw": {
        "workerDirectory": "public"
    }
}
